{
  "name": "codenamephp/platform.di",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Bastian Schwarz",
      "email": "bastian@codename-php.de"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.6",
    "codenamephp/platform.core": "^1",
    "php-di/php-di": "^5"
  },
  "require-dev": {
    "codenamephp/platform.test": "^1",
    "doctrine/cache": "~1.4",
    "pdepend/pdepend": "^2",
    "squizlabs/php_codesniffer": "^2",
    "phploc/phploc": "^3",
    "phpmd/phpmd": "^2",
    "sebastian/phpcpd": "^2",
    "theseer/phpdox": "^0.8"
  },
  "autoload": {
    "classmap": ["src/main/php"]
  },
  "autoload-dev": {
    "psr-0": {
      "de\\codenamephp\\": ["src/main/php", "src/test/php"],
      "": "vendor/pdepend/pdepend/src/main/php"
    }
  },
  "archive": {
    "exclude": ["/src/test"]
  }
}
